    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^1.3.1",
    "koa-router": "^7.0.0",
    "koa-static-plus": "^0.1.1",
    "koa-views": "^5.0.2",
    "lodash": "^4.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
